{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "noImplicitAny": false,
    "sourceMap": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "outDir": "lib",
    "jsx": "preserve"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
